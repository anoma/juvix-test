module Example;

import Stdlib.Prelude open;
import Test.JuvixUnit open;
import Stdlib.Data.Nat.Ord open;

tests : List Test :=
  [testCase
    "1 == 1"
    (assertEqual "1 /= 1" 1 1); testCase
    "not (1 == 1)"
    (assertFalse "1 == 1" (1 == 1))];

main : IO :=
  runTestSuite
    (testSuite (name := "Example"; tests := tests));
